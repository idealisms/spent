{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strict": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "outDir": "build"
  },
  "exclude": [
    "build",
    "config",
    "node_modules",
    "dev_server.js"
  ]
}
