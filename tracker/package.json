{
  "name": "tracker",
  "version": "0.1.0",
  "description": "Web GUI",
  "main": "index.js",
  "license": "UNLICENSED",
  "scripts": {
    "build:webpack": "NODE_ENV=production webpack --mode=production --config config/webpack.prod.config.js --progress --profile",
    "build": "npm run clean && npm run build:webpack",
    "clean": "rimraf build",
    "deploy": "npm run build && scp -r build/* google_cloud.tailscale:~/www/spent/",
    "watch": "NODE_ENV=development webpack --mode=development --config config/webpack.dev.config.js --watch --progress --profile",
    "start": "npm run clean && webpack-dev-server --mode=development --config config/webpack.dev.config.js --progress --profile",
    "test": "npm run clean && jest",
    "test:watch": "npm test -- --watch",
    "eslint-prettier-check": "eslint-config-prettier app/**/*.ts",
    "prettier": "prettier --write ."
  },
  "bugs": {
    "url": "https://github.com/idealisms/spent/issues"
  },
  "homepage": "https://github.com/idealisms/spent/",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "setupFiles": [
      "./config/test.shim.js"
    ],
    "setupFilesAfterEnv": [
      "./config/test.setup.ts"
    ],
    "testRegex": "/*.spec.(ts|tsx|js)$",
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://www.example.com/"
    }
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/classnames": "^2.2.7",
    "@types/google.visualization": "^0.0.46",
    "@types/jest": "29",
    "@types/memoize-one": "^4.1.0",
    "@types/prop-types": "^15.5.8",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "5",
    "@types/react-select": "5",
