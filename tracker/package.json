{
  "name": "tracker",
  "version": "0.1.0",
  "description": "Web GUI",
  "main": "index.js",
  "license": "UNLICENSED",
  "scripts": {
    "build:webpack": "NODE_ENV=production webpack --mode=production --config config/webpack.prod.config.js --progress --profile --colors",
    "build": "npm run clean && npm run build:webpack",
    "clean": "rimraf build",
    "deploy": "npm run build && scp -r build/* pi3:~/projects/quant/spent/",
    "watch": "NODE_ENV=development webpack --mode=development --config config/webpack.dev.config.js --watch --progress --profile --colors",
    "start": "npm run clean && webpack-dev-server --mode=development --config config/webpack.dev.config.js --progress --profile --colors",
    "start:deno": "npm run clean && mkdir build && NODE_ENV=development deno run --allow-net --allow-env --allow-run --allow-read=build server/server.ts",
    "start:deno:prod": "NODE_ENV=production deno run --allow-net --allow-env --allow-read=build server/server.ts",
    "test": "npm run clean && jest",
    "test:watch": "npm test -- --watch",
    "prettier": "prettier --write . && eslint -c .eslintrc.js --fix --ext .ts,.tsx app"
  },
  "bugs": {
    "url": "https://github.com/idealisms/spent/issues"
  },
  "homepage": "https://github.com/idealisms/spent/",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "setupFiles": [
      "./config/test.shim.js",
      "./config/test.setup.js"
    ],
    "setupTestFrameworkScriptFile": "./config/test.setup.ts",
    "testRegex": "/*.spec.(ts|tsx|js)$",
    "testURL": "http://www.example.com/"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.1.6",
    "@types/google.visualization": "^0.0.46",
    "@types/jest": "^22.0.1",
    "@types/memoize-one": "^4.1.0",
    "@types/prop-types": "^15.5.8",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.1",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-select": "^2.0.19",
    "@types/react-test-renderer": "^16.0.3",
    "@types/redux-devtools": "^3.0.45",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-mock-store": "^0.0.13",
    "@types/sha.js": "^2.4.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "dropbox": "^5.2.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^7.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.5",
    "express": "^4.15.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "prettier": "^2.1.1",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.2.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.5.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "sw-precache-webpack-plugin": "^1.0.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.3.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "@date-io/moment": "^1.0.1",
    "@material-ui/core": "^4.8.2",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/pickers": "^3.2.8",
    "awesome-typescript-loader": "^5.2.1",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.6.1",
    "css-loader": "^3.0.0",
    "emotion": "^10.0.9",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "memoize-one": "^5.0.0",
    "moment": "^2.23.0",
    "prop-types": "^15.6.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-google-charts": "^3.0.10",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.1.2",
    "react-select": "^2.4.4",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "sha.js": "^2.4.11",
    "style-loader": "^0.21.0"
  },
  "resolutions": {
    "dot-prop": "^5.1.1",
    "selfsigned": "^1.10.8"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  }
}
